function esppp()



--[[
pwn#2203
--]]
	Instance.new("ScreenGui")
  local Outlines = true
	local Folder = Instance.new("Folder", game:GetService("CoreGui"))
	Folder.Name = "hi_litez_boxes"

AddBox = function(Character)
local box0 = Instance.new("SelectionBox", Folder)
box0.Transparency = 0
box0.SurfaceTransparency = 1
box0.SurfaceColor3 = Color3.fromRGB(67, 224, 97)
box0.LineThickness = 0.05
box0.Color3 = Color3.fromRGB(67, 224, 97)
box0.Adornee = Character
	end


 
	for i, v in ipairs(game:GetService("Players"):GetPlayers()) do
		if v ~= game:GetService("Players").LocalPlayer then
			v.CharacterAdded:Connect(function(Character)
				if Outlines == true then
          AddBox(Character)
					--AddOutline(Character)
				end
				if NameTags == true then
					--AddNameTag(Character)
					--AddNameTag2(Character)
          --AddNameTag3(Character)
				end
			end)

			if v.Character then
				if Outlines == true then
            AddBox(v.Character)
					--AddOutline(v.Character)
				end
				if NameTags == true then
					--AddNameTag(v.Character)
					--AddNameTag2(v.Character)
          --AddNameTag3(v.Character)
				end
			end
		end
	end


	game:GetService("Players").PlayerAdded:Connect(function(Player)
		Player.CharacterAdded:Connect(function(Character)
			if Outlines == true then
          AddBox(Character)
				--AddOutline(Character)
			end
			if NameTags == true then
				--AddNameTag(Character)
				--AddNameTag2(Character)
       	--AddNameTag3(Character)
			end
		end)
	end)
end

esppp()
