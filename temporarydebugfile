local temp = AZX:CreateUI("ESP Manager V6")



local body1 = temp:CreateBody("body")
addButton("Hi-Lite V2", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/exigeoni/wl0/main/Hi-Lite_V2"))()
end)

addButton("Refresh Hi-Lite V2", function()
	game.CoreGui.hi_litez:Destroy()
	wait(.35)
	loadstring(game:HttpGet("https://raw.githubusercontent.com/exigeoni/wl0/main/Hi-Lite_V2"))()
	print("Refreshed")
end)

addButton("Destroy Hi-Lite V2", function()
	game.CoreGui.hi_litez:Destroy()
end)

local body2 = temp:CreateBody("body2")

_G.SelectedEsp = "N/A"

addButton("Set V2", function()
	_G.SelectedEsp = "V2"
end)
addButton("Set V2.5", function()
	_G.SelectedEsp = "V2.5"
end)
addButton("Set V3", function()
	_G.ConsoleMsg("V3 Doesnt Support Loops Yet", _G.clb_fatal)
end)

addButtonAndInputSection2("Refresh Time Here (Seconds)", "Start V2 Loop", function()
	local TheBoxValue2 = _G.globalBOXINPUT2

	local function loopp2()
	
	if _G.SelectedEsp == "V2" then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/exigeoni/wl0/main/Hi-Lite_V2"))()
	end
	
	if _G.SelectedEsp == "V2.5" then
		loadstring(game:HttpGet('https://raw.githubusercontent.com/exigeoni/wl0/main/Hi-Lite_V3-WASTELANDTYCOON_EDITION'))()
	end
	
	
		wait(TheBoxValue2)
		game.CoreGui.hi_litez:Destroy()
	end

	while true do
		loopp2()
		wait(.05)
	end

end)


local body3 = temp:CreateBody("body3")
addButton("Unnamed ESP", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua'))()
end)


