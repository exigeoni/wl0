		end

		function addToggleButton(text, bscript, bscript2)



			local bscript = bscript or function() end
			local bscript2 = bscript2 or function() end

			local toggleb = false

			local nbuton = Instance.new("TextButton")
			local tbUIStroke = Instance.new("UIStroke")
