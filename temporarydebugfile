	if TheBoxValue == "tww" then
		loadstring(game:HttpGet('https://raw.githubusercontent.com/exigeoni/wl0/main/Hi-Lite_V3-BETA-TWW'))()
	end

	if TheBoxValue == "V2.5" then
		loadstring(game:HttpGet('https://raw.githubusercontent.com/exigeoni/wl0/main/Hi-Lite_V3-WASTELANDTYCOON_EDITION'))()	--originally meant for wasteland tycoon but is also a very good V2 alternative
	end

end)

addButton("Destroy Old Hi-Lite Folder", function()
	game.CoreGui.hi_litez:Destroy()
end)


