local re = Instance.new("TextLabel")
re.Name = "reminder"
re.Parent = mainframe2
re.BackgroundColor3 = Color3.fromRGB(9,9,9)
re.BorderSizePixel = 0
re.BackgroundTransparency = 0
re.Position = UDim2.new(0,0,0.028,0)
re.Size = UDim2.new(0,249,0,33)
re.Font = F_ss
re.TextColor3 = Color3.fromRGB(255,255,255)
re.TextScaled = true
re.Text = "Info Is Printed To Roblox Console"
re.ZIndex = 1

PSI2.MouseButton1Up:Connect(function()
	local selectedplayer = PSI.Text
	local pathh = game.Players:WaitForChild(selectedplayer)
	local charpath = game.Workspace:WaitForChild(selectedplayer)
	local charhum = charpath:WaitForChild("Humanoid")
	local playerhealth = charhum.Health
	local plyrzbackpack = pathh:WaitForChild("Backpack")

	local playeritemz = plyrzbackpack:GetChildren()
	local plyritemTable = {playeritemz}

	local list2print = Instance.new("StringValue")
	for i, v in pairs(plyritemTable) do
		list2print = v
	end

	
	
	print(selectedplayer)
	print("-------------")
	print(playerhealth)
	print(list2print)
	print("-------------")

	--ph.Text = playerhealth
	--pi.Text = playeritemz2
end)


