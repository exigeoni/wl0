local sg = Instance.new("ScreenGui")
sg.Name = "ivp-SG"
sg.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
sg.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
sg.ResetOnSpawn = false
local mainframe = Instance.new("Frame")
mainframe.Name = "ItemViewPanel"
mainframe.Parent = sg
mainframe.BackgroundColor3 = Color3.fromRGB(25,25,25)
mainframe.BorderSizePixel = 0
mainframe.BackgroundTransparency = 0
mainframe.Position = UDim2.new(0.33,0,0.631,0)
mainframe.Size = UDim2.new(0,175,0,220)
mainframe.ZIndex = 0
mainframe.Active = true
mainframe.Draggable = true

local mainframe2 = Instance.new("Frame")
mainframe2.Name = "ItemViewPanel2"
mainframe2.Parent = mainframe
mainframe2.BackgroundColor3 = Color3.fromRGB(25,25,25)
